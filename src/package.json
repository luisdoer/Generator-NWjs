{
  "main": "app.html",
  "name": "{{appName}}",
  "version": "1.4.3",
  "app-id": "com.{{githubUsername}}.{{appFolder}}",
  "chromium-args": "--disable-setuid-sandbox",
  "window": {
    "width": 800,
    "height": 600,
    "title": "{{appName}}",
    "icon": "images/icon.png",
    "toolbar": false,
    "show": false
  },
  "webkit": {
    "plugin": true
  },
  "dependencies": {
    "async": "^1.0.0",
    "auto-launch": "^0.1.18",
    "jfs": "^0.2.5",
    "request": "^2.56.0",
    "semver": "^4.3.5"
  },
  "manifestUrl": "https://raw.githubusercontent.com/{{githubUsername}}/{{githubRepoName}}/master/src/package.json",
  "packages": {
    "osx64": "https://github.com/{{githubUsername}}/{{githubRepoName}}/releases/download/v1.4.3/{{appFolder}}.dmg",
    "win32": "https://github.com/{{githubUsername}}/{{githubRepoName}}/releases/download/v1.4.3/{{appFolder}}Setup.exe",
    "linux32": "https://github.com/{{githubUsername}}/{{githubRepoName}}/releases/download/v1.4.3/{{appFolder}}_linux32.deb",
    "linux64": "https://github.com/{{githubUsername}}/{{githubRepoName}}/releases/download/v1.4.3/{{appFolder}}_linux64.deb"
  }
}
